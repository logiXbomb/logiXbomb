{
  "apiVersion": "extensions/v1beta1",
  "kind": "Deployment",
  "metadata": {
    "name": "testitout"
  },
  "spec": {
    "replicas": 1,
    "template": {
      "metadata": {
        "labels": {
          "app": "testitout"
        }
      },
      "spec": {
        "containers": [
          {
            "name": "testitout",
            "ports": [
              {
                "containerPort": 80
              }
            ],
            "image": "test:latest",
            "command": [
              "/bin/sh",
              "-c",
              "/./main"
            ],
            "imagePullPolicy": "Never",
            "readinessProbe": {
              "httpGet": {
                "path": "/",
                "port": 80
              },
              "initialDelaySeconds": 10,
              "periodSeconds": 30,
              "timeoutSeconds": 10
            }
          }
        ]
      }
    }
  }
}